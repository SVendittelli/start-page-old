{
  "name": "start-page",
  "version": "0.1.0",
  "private": true,
  "description": "My personal start page.",
  "keywords": [
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SVendittelli/start-page.git"
  },
  "license": "MIT",
  "author": {
    "name": "Sam Vendittelli",
    "email": "sam.vendittelli@hotmail.com",
    "url": "https://vendittelli.co.uk"
  },
  "scripts": {
    "build": "react-scripts build",
    "commit": "git-cz",
    "prepare": "is-ci || husky install",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "npm run test --silent -- --coverage"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.7.1",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "12.20.41",
    "@types/react": "16.14.21",
    "@types/react-dom": "16.9.14",
    "@types/react-redux": "7.1.21",
    "is-ci": "3.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-scripts": "5.0.1",
    "typescript": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@commitlint/cz-commitlint": "16.0.0",
    "commitizen": "4.2.4",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-packagejson": "2.2.15",
    "prettier-plugin-sh": "0.8.1"
  },
  "engines": {
    "node": "^16",
    "npm": ">= 8.0.0"
  }
}
